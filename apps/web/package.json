{
  "name": "web",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "check-types": "tsc --noEmit",
    "tslint": "pnpm lint && pnpm check-types",
    "start": "next start",
    "shad": "pnpx shadcn@latest add",
    "zod": "pnpm exec supabase-to-zod --input src/server/db/schema/default.ts --output src/server/db/zod.ts",
    "types:local": "pnpm exec supabase gen types --lang=typescript --local --schema public> src/server/db/schema/default.ts && pnpm run zod",
    "types:remote": "pnpm exec supabase gen types --lang=typescript --linked --schema public> src/server/db/schema/default.ts && pnpm run zod",
    "seed:local": "pnpm exec supabase db dump -f supabase/seed.sql --local --data-only --exclude public.user,auth.users,auth.identities,auth.audit_log_entries,auth.flow_state,auth.sessions,auth.mfa_amr_claims,auth.one_time_tokens,auth.refresh_tokens",
    "seed:remote": "pnpm exec supabase db dump -f supabase/seed.sql --linked --data-only --exclude public.user,auth.users,auth.identities,auth.audit_log_entries,auth.flow_state,auth.sessions,auth.mfa_amr_claims,auth.one_time_tokens,auth.refresh_tokens",
    "db": "pnpm exec supabase db",
    "migration": "pnpm exec supabase migration"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@supabase/ssr": "^0.5.1",
    "@supabase/supabase-js": "^2.45.4",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tanstack/react-query": "^5.59.13",
    "@tanstack/react-query-devtools": "^5.59.13",
    "@trpc/client": "^11.0.0-rc.446",
    "@trpc/react-query": "^11.0.0-rc.446",
    "@trpc/server": "^11.0.0-rc.446",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "geist": "^1.3.0",
    "lucide-react": "^0.441.0",
    "next": "^15.0.3",
    "next-themes": "^0.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^4.1.2",
    "react-hook-form": "^7.53.2",
    "server-only": "^0.0.1",
    "superjson": "^2.2.1",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.0.0",
    "zod": "^3.23.8",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "postcss": "^8.4.39",
    "supabase": "^1.223.10",
    "supabase-to-zod": "^1.0.7",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.6.2"
  }
}
